{
    "name": "hey",
    "displayName": "hey",
    "publisher": "nab",
    "icon": "https://raw.githubusercontent.com/nbstr/hey-vscode/master/icon/icon256.png",
    "description": "Ask code-realted questions right from your IDE.",
    "version": "0.1.3",
    "repository": "https://github.com/nbstr/hey-vscode",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHey"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHey",
                "title": "ðŸ‘‹ Hey! Ask anything about your code."
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "extension.sayHey",
                    "group": "HeyGroup@1"
                }
            ]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "request-promise": "^4.2.2"
    }
