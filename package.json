{
    "name": "vscode",
    "displayName": "Hey",
    "publisher": "hey",
    "author": {
        "name": "Nabil Kamel"
    },
    "icon": "icon/icon256.png",
    "description": "Ask code-related questions right from your IDE. Share code snippets and find help the fastest way possible.",
    "keywords": [
        "snippet",
        "share",
        "help",
        "context",
        "community"
    ],
    "version": "1.0.0",
    "license": "UNLICENSED",
    "qna": "https://t.me/heysociety",
    "repository": {
        "type": "git",
        "url": "https://github.com/nbstr/hey-vscode.git"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Debuggers",
        "Extension Packs"
    ],
    "activationEvents": [
        "onCommand:extension.sayHey"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHey",
                "title": "ðŸ‘‹ Hey! Ask anything about your code."
            }
        ],
        "menus": {
